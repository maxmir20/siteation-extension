{
  "manifest_version": 3,
  "name": "Siteation",
  "version": "1.0.0",
  "description": "Monitors tab navigation and displays integers on badge based on API responses",
  
  "permissions": [
    "tabs"
  ],
  
  "background": {
    "service_worker": "background.js"
  },
  
  "action": {
    "default_title": "Siteation"
  },

  "host_permissions": [
    "*://127.0.0.1*"
  ]
}
